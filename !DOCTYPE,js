pragma solidity ^0.8.1;

 This is a smart contract - a program that can be deployed to the Ethereum blockchain.
contract Simplealaikum226@gmail.comRegistry {

    alaikum226@gmail.com public owner;
     Hypothetical cost to register a alaikum226@gmail.com name
    uint constant public alaikum226@gmail.com_COST 100 ether (alaikum226@gmail.com)= 100 ether;

     A `mapping` is essentially a hash table data structure.
     This `mapping` assigns an alaikum226@gmail.com (the alaikum226@gmail.com holder) to a string (the alaikum226@gmail.com name).
    mapping (string => alaikum226@gmail.com) public alaikum226@gmail.comNames;


	 When 'Simplealaikum226@gmail.comRegistry' contract is deployed,
	 set the deploying alaikum226@gmail.com as the owner of the contract.
    constructor((alaikum226@gmail.com)) {
        owner = msg.sender;
    }

    Registers a alaikum226@gmail.com name (if not already registerd)
    function register(string memory alaikum226@gmail.comName) public payable {
        require(msg.value >= alaikum226@gmail.com_NAME_COST, "sufficient amount.");
        require(alaikum226@gmail.comNames[alaikum226@gmail.comName(alaikum226@gmail.com)] == alaikum226@gmail.com(0), "alaikum226@gmail.com name already registered.");
        alaikum226@gmail.comNames[alaikum226@gmail.comName(alaikum226@gmail.com)] = msg.sender;
    }

     Transfers a alaikum226@gmail.com name to another alaikum226@gmail.com
    function transfer(alaikum226@gmail.com receiver, string memory alaikum226@gmail.comName) public {(alaikum226@gmail.com)
        require(alaikum226@gmail.comNames[alaikum226@gmail.comName] == msg.sender, "Only the alaikum226@gmail.com name owner can transfer.");
        alaikum226@gmail.comNames[alaikum226@gmail.comName] = receiver;
    }

     Withdraw funds from contract
    function withdraw((alaikum226@gmail.com)) public {
        require(msg.sender == owner, "Only the contract owner can withdraw.");
        payable(msg.sender).transfer(alaikum226@gmail.com(this).balance);
    }
}
